<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hibernate.beans">
   <class name="Order" table="order">
    <id name="contactNo" column="ContactNo" />
        <property name="customerName" column="customerName" />
        <property name="tableNo" column="tableNo" />
        <property name="dishName" column="dishName" />
        <property name="price" column="price" />
        <property name="review" column="review" />
    </class>
</hibernate-mapping>    

